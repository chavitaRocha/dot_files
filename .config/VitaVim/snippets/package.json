{
    "name": "vita-snippets",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": [
                    "plaintext",
                    "markdown",
                    "tex",
                    "html",
                    "global",
                    "all"
                ],
                "path": "./global.json"
            },
            {
                "language": ["csharp", "cs"],
                "path": "./csharp.json"
            },
            {
                "language": [ "scss", "css" ],
                "path": "./css.json"
            }
        ]
    }
}

